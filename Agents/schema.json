{
    "openapi": "3.0.0",
    "info": {
        "title": "Insurance Claims Automation API",
        "version": "1.0.0",
        "description": "APIs for managing insurance claims by pulling a list of open claims, identifying outstanding paperwork for each claim, and sending reminders to policy holders."
    },
    "paths": {
        "/event": {
            "get": {
                "summary": "Get a list of all events",
                "description": "Get the list of all created events.",
                "operationId": "getAllEvents",
                "responses": {
                    "200": {
                        "description": "Gets the list of all events",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "eventId": {
                                                "type": "string",
                                                "description": "Unique ID of the event."
                                            },
                                            "eventDate": {
                                                "type": "string",
                                                "description": "Event date."
                                            },
                                            "eventTime": {
                                                "type": "string",
                                                "description": "The time of the event."
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}